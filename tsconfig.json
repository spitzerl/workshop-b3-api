{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"strict": true,
		"outDir": "dist",
		"baseUrl": ".",
		"types": ["node", "express"]
	},
	"include": ["**/*.ts"],
	"exclude": ["node_modules", "dist"]
}
